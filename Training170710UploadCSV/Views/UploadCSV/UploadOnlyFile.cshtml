
@{
    ViewBag.Title = "UploadOnlyFile";
}

<h2>UploadOnlyFile</h2>

<form action="UploadOnlyFile" method="post" enctype="multipart/form-data">
    <div class="form-horizontal">
        <hr />
        <div class="form-group">
            <label for="file" class="control-label col-md-2">Filename</label>
            <div class="col-md-10">
                <input type="file" name="file" id="file" class="form-control" />
            </div>
        </div>
        <div class="form-group">
            <div class="col-md-offset-2 col-md-10">
                <input id="btnSubmit" type="submit" value="Create" class="btn btn-default" />
            </div>
        </div>
    </div>
</form>

